{
  "name": "template-vue",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen-api": "node ./scripts/generate-api.mjs",
    "postinstall": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^13.1.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cqfe/eslint-config": "^1.3.8",
    "@cqfe/generate-api": "0.11.19",
    "@cqfe/prettier-config": "^1.3.10",
    "@cqfe/stylelint-config": "^1.3.4",
    "@vitejs/plugin-vue": "^4.6.2",
    "@types/node": "^22.14.1",
    "sass": "^1.69.7",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^5.4.18",
    "husky": "^9.1.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": "@cqfe/prettier-config",
  "eslintConfig": {
    "extends": "@cqfe/eslint-config"
  },
  "stylelint": {
    "extends": "@cqfe/stylelint-config"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier -c --write"
    ]
  }
}
